    "    budgets.sort()\n",
    "    left,right = 0, budgets[len(budgets)-1]\n",
    "    while(left<right):\n",
    "        mid = (left+right+1) // 2\n",
    "        if (calculate(budgets,M,mid) !=0):\n",
    "            left = mid\n",
    "        else:\n",
    "            right = mid -1\n",
    "    return left"
